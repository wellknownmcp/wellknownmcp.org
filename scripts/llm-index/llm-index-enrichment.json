  "version_routing_logic": {
    "agent_detection": {
      "user_agent_patterns": ["bot", "crawler", "spider", "claude", "gpt", "anthropic"],
      "referrer_patterns": ["claude.ai", "openai.com", "anthropic.com", "perplexity.ai"],
      "default_route": "agent",
      "confidence_threshold": 0.8
    },
    "technical_detection": {
      "referrer_patterns": ["github.com", "stackoverflow.com", "hackernews", "dev.to"],
      "query_patterns": ["implementation", "api", "docs", "integration"],
      "default_route": "tech",
      "confidence_threshold": 0.7
    },
    "business_detection": {
      "referrer_patterns": ["linkedin.com", "twitter.com/business", "medium.com"],
      "query_patterns": ["roi", "business", "case-study", "enterprise"],
      "default_route": "business",
      "confidence_threshold": 0.6
    },
    "fallback_logic": {
      "first_visit": "simple",
      "return_visit": "maintain_previous_version",
      "uncertain": "simple"
    }
  },

